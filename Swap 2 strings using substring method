package Practice;

// Returns the swapped string using the substring method

public class SwapTwoStringsUsingSubstring {

	public static void main(String[] args) {
	
		String str1 = "Rahul";
		String str2 = "Shetty";
		String str3 = str1+str2;
		
		str1 = str3.substring(str2.length()-1, str3.length());
		System.out.println(str1);
		str2 = str3.substring(0,str2.length()-1);
		System.out.println(str2);

	}

}
